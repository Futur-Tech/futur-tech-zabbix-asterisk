<?xml version="1.0" encoding="UTF-8"?>
<zabbix_export>
    <version>5.2</version>
    <date>2021-03-13T11:22:50Z</date>
    <groups>
        <group>
            <name>Templates</name>
        </group>
        <group>
            <name>Templates/Applications</name>
        </group>
    </groups>
    <templates>
        <template>
            <template>Asterisk PBX</template>
            <name>Asterisk PBX</name>
            <description>Asterisk PBX monitoring template by InitZero

https://github.com/ugoviti/zabbix-templates/tree/master/asterisk
Version: 20210313</description>
            <groups>
                <group>
                    <name>Templates</name>
                </group>
                <group>
                    <name>Templates/Applications</name>
                </group>
            </groups>
            <applications>
                <application>
                    <name>Asterisk PBX</name>
                </application>
            </applications>
            <items>
                <item>
                    <name>Concurrent Active Calls</name>
                    <key>check.asterisk[calls.active]</key>
                    <history>30d</history>
                    <applications>
                        <application>
                            <name>Asterisk PBX</name>
                        </application>
                    </applications>
                    <triggers>
                        <trigger>
                            <expression>{last(#3)}&gt;={$ASTERISK_CALLS_ACTIVE_WARN}</expression>
                            <name>{$ASTERISK_CALLS_ACTIVE_WARN} concurrent active calls detected</name>
                            <priority>WARNING</priority>
                        </trigger>
                    </triggers>
                </item>
                <item>
                    <name>Longest Call Channel Name</name>
                    <key>check.asterisk[calls.longest.channel]</key>
                    <history>30d</history>
                    <trends>0</trends>
                    <value_type>TEXT</value_type>
                    <applications>
                        <application>
                            <name>Asterisk PBX</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>Longest Call Duration</name>
                    <key>check.asterisk[calls.longest.duration]</key>
                    <history>30d</history>
                    <units>uptime</units>
                    <applications>
                        <application>
                            <name>Asterisk PBX</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>Processed Calls</name>
                    <key>check.asterisk[calls.processed]</key>
                    <history>30d</history>
                    <applications>
                        <application>
                            <name>Asterisk PBX</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>IAX2 Peers Offline</name>
                    <key>check.asterisk[iax2.peers.offline]</key>
                    <delay>5m</delay>
                    <history>30d</history>
                    <applications>
                        <application>
                            <name>Asterisk PBX</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>IAX2 Peers Online</name>
                    <key>check.asterisk[iax2.peers.online]</key>
                    <delay>5m</delay>
                    <history>30d</history>
                    <applications>
                        <application>
                            <name>Asterisk PBX</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>IAX2 Trunks Offline</name>
                    <key>check.asterisk[iax2.trunks.offline]</key>
                    <delay>5m</delay>
                    <history>30d</history>
                    <applications>
                        <application>
                            <name>Asterisk PBX</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>IAX2 Trunks Online</name>
                    <key>check.asterisk[iax2.trunks.online]</key>
                    <delay>5m</delay>
                    <history>30d</history>
                    <applications>
                        <application>
                            <name>Asterisk PBX</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>Asterisk Uptime after reload</name>
                    <key>check.asterisk[lastreload]</key>
                    <delay>10m</delay>
                    <history>30d</history>
                    <units>uptime</units>
                    <applications>
                        <application>
                            <name>Asterisk PBX</name>
                        </application>
                    </applications>
                    <triggers>
                        <trigger>
                            <expression>{last()}&lt;10m</expression>
                            <name>Asterisk service has been reloaded (uptime &lt; 10m)</name>
                            <priority>INFO</priority>
                            <description>A configuration change was made less than 10 minutes ago</description>
                            <manual_close>YES</manual_close>
                        </trigger>
                    </triggers>
                </item>
                <item>
                    <name>PJSIP Endpoints Offline</name>
                    <key>check.asterisk[pjsip.endpoints.offline]</key>
                    <delay>5m</delay>
                    <history>30d</history>
                    <applications>
                        <application>
                            <name>Asterisk PBX</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>PJSIP Endpoints Online</name>
                    <key>check.asterisk[pjsip.endpoints.online]</key>
                    <delay>5m</delay>
                    <history>30d</history>
                    <applications>
                        <application>
                            <name>Asterisk PBX</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>PJSIP Trunks Offline</name>
                    <key>check.asterisk[pjsip.trunks.offline]</key>
                    <delay>5m</delay>
                    <history>30d</history>
                    <applications>
                        <application>
                            <name>Asterisk PBX</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>PJSIP Trunks Online</name>
                    <key>check.asterisk[pjsip.trunks.online]</key>
                    <delay>5m</delay>
                    <history>30d</history>
                    <applications>
                        <application>
                            <name>Asterisk PBX</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>Asterisk Service Status</name>
                    <key>check.asterisk[service.status]</key>
                    <history>30d</history>
                    <trends>0</trends>
                    <value_type>TEXT</value_type>
                    <applications>
                        <application>
                            <name>Asterisk PBX</name>
                        </application>
                    </applications>
                    <triggers>
                        <trigger>
                            <expression>{str(Down,#1)}=1</expression>
                            <name>Asterisk service is DOWN</name>
                            <priority>DISASTER</priority>
                        </trigger>
                    </triggers>
                </item>
                <item>
                    <name>SIP Peers Offline</name>
                    <key>check.asterisk[sip.peers.offline]</key>
                    <delay>5m</delay>
                    <history>30d</history>
                    <applications>
                        <application>
                            <name>Asterisk PBX</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>SIP Peers Online</name>
                    <key>check.asterisk[sip.peers.online]</key>
                    <delay>5m</delay>
                    <history>30d</history>
                    <applications>
                        <application>
                            <name>Asterisk PBX</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>SIP Trunks Offline</name>
                    <key>check.asterisk[sip.trunks.offline]</key>
                    <delay>5m</delay>
                    <history>30d</history>
                    <applications>
                        <application>
                            <name>Asterisk PBX</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>SIP Trunks Online</name>
                    <key>check.asterisk[sip.trunks.online]</key>
                    <delay>5m</delay>
                    <history>30d</history>
                    <applications>
                        <application>
                            <name>Asterisk PBX</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>Asterisk Uptime</name>
                    <key>check.asterisk[systemuptime]</key>
                    <delay>10m</delay>
                    <history>30d</history>
                    <units>uptime</units>
                    <applications>
                        <application>
                            <name>Asterisk PBX</name>
                        </application>
                    </applications>
                    <triggers>
                        <trigger>
                            <expression>{last()}&lt;10m</expression>
                            <name>Asterisk service has been restarted (uptime &lt; 10m)</name>
                            <priority>INFO</priority>
                            <description>Uptime is less than 10 minutes</description>
                            <manual_close>YES</manual_close>
                        </trigger>
                    </triggers>
                </item>
                <item>
                    <name>Asterisk Version</name>
                    <key>check.asterisk[version]</key>
                    <delay>10m</delay>
                    <history>30d</history>
                    <trends>0</trends>
                    <value_type>TEXT</value_type>
                    <applications>
                        <application>
                            <name>Asterisk PBX</name>
                        </application>
                    </applications>
                    <triggers>
                        <trigger>
                            <expression>{diff()}=1 and {strlen()}&gt;0</expression>
                            <name>Asterisk version has changed (new version: {ITEM.VALUE})</name>
                            <priority>INFO</priority>
                            <description>Asterisk version has changed. Ack to close.</description>
                            <manual_close>YES</manual_close>
                        </trigger>
                    </triggers>
                </item>
            </items>
            <discovery_rules>
                <discovery_rule>
                    <name>Discovery IAX2 Trunks</name>
                    <key>discovery.asterisk[discovery.iax2.registry]</key>
                    <delay>1h</delay>
                    <lifetime>7d</lifetime>
                    <item_prototypes>
                        <item_prototype>
                            <name>IAX2 Trunk Status for {#USERNAME}@{#HOST}</name>
                            <key>check.asterisk[iax2.registry,{#USERNAME}]</key>
                            <trends>0</trends>
                            <value_type>TEXT</value_type>
                            <applications>
                                <application>
                                    <name>Asterisk PBX</name>
                                </application>
                            </applications>
                            <trigger_prototypes>
                                <trigger_prototype>
                                    <expression>{regexp(Registered,#3)}=0</expression>
                                    <name>IAX2 Trunk not registered {#USERNAME}@{#HOST}</name>
                                    <priority>HIGH</priority>
                                </trigger_prototype>
                            </trigger_prototypes>
                        </item_prototype>
                    </item_prototypes>
                </discovery_rule>
                <discovery_rule>
                    <name>Discovery PJSIP Trunks</name>
                    <key>discovery.asterisk[discovery.pjsip.registry]</key>
                    <delay>1h</delay>
                    <lifetime>7d</lifetime>
                    <item_prototypes>
                        <item_prototype>
                            <name>PJSIP Trunk Status for {#ENDPOINT}/sip:{#USERNAME}@{#HOST}</name>
                            <key>check.asterisk[pjsip.registry,{#ENDPOINT}]</key>
                            <trends>0</trends>
                            <value_type>TEXT</value_type>
                            <applications>
                                <application>
                                    <name>Asterisk PBX</name>
                                </application>
                            </applications>
                            <trigger_prototypes>
                                <trigger_prototype>
                                    <expression>{regexp(Avail,#3)}=0</expression>
                                    <name>PJSIP Trunk not registered {#ENDPOINT}/sip:{#USERNAME}@{#HOST}</name>
                                    <priority>HIGH</priority>
                                </trigger_prototype>
                            </trigger_prototypes>
                        </item_prototype>
                    </item_prototypes>
                </discovery_rule>
                <discovery_rule>
                    <name>Discovery SIP Trunks</name>
                    <key>discovery.asterisk[discovery.sip.registry]</key>
                    <delay>1h</delay>
                    <lifetime>7d</lifetime>
                    <item_prototypes>
                        <item_prototype>
                            <name>SIP Trunk Status for {#USERNAME}@{#HOST}</name>
                            <key>check.asterisk[sip.registry,{#USERNAME}]</key>
                            <history>30d</history>
                            <trends>0</trends>
                            <value_type>TEXT</value_type>
                            <applications>
                                <application>
                                    <name>Asterisk PBX</name>
                                </application>
                            </applications>
                            <trigger_prototypes>
                                <trigger_prototype>
                                    <expression>{regexp(Registered,#3)}=0</expression>
                                    <name>SIP Trunk not registered {#USERNAME}@{#HOST}</name>
                                    <priority>HIGH</priority>
                                </trigger_prototype>
                            </trigger_prototypes>
                        </item_prototype>
                    </item_prototypes>
                </discovery_rule>
            </discovery_rules>
            <macros>
                <macro>
                    <macro>{$ASTERISK_CALLS_ACTIVE_WARN}</macro>
                    <value>20</value>
                    <description>Alarm when reaching max active calls threshold</description>
                </macro>
                <macro>
                    <macro>{$ASTERISK_CALLS_DURATION_WARN}</macro>
                    <value>7200</value>
                    <description>Alarm when reaching call duration time (in seconds)</description>
                </macro>
            </macros>
            <dashboards>
                <dashboard>
                    <name>Asterisk PBX</name>
                    <widgets>
                        <widget>
                            <type>GRAPH_CLASSIC</type>
                            <width>12</width>
                            <height>5</height>
                            <fields>
                                <field>
                                    <type>GRAPH</type>
                                    <name>graphid</name>
                                    <value>
                                        <name>Asterisk Concurrent Active Calls</name>
                                        <host>Asterisk PBX</host>
                                    </value>
                                </field>
                            </fields>
                        </widget>
                        <widget>
                            <type>GRAPH_CLASSIC</type>
                            <x>12</x>
                            <width>12</width>
                            <height>5</height>
                            <fields>
                                <field>
                                    <type>GRAPH</type>
                                    <name>graphid</name>
                                    <value>
                                        <name>Asterisk Longest Call Duration</name>
                                        <host>Asterisk PBX</host>
                                    </value>
                                </field>
                            </fields>
                        </widget>
                        <widget>
                            <type>GRAPH_CLASSIC</type>
                            <y>5</y>
                            <width>12</width>
                            <height>7</height>
                            <fields>
                                <field>
                                    <type>GRAPH</type>
                                    <name>graphid</name>
                                    <value>
                                        <name>Asterisk Peers Status</name>
                                        <host>Asterisk PBX</host>
                                    </value>
                                </field>
                            </fields>
                        </widget>
                        <widget>
                            <type>GRAPH_CLASSIC</type>
                            <x>12</x>
                            <y>5</y>
                            <width>12</width>
                            <height>7</height>
                            <fields>
                                <field>
                                    <type>GRAPH</type>
                                    <name>graphid</name>
                                    <value>
                                        <name>Asterisk Trunks Status</name>
                                        <host>Asterisk PBX</host>
                                    </value>
                                </field>
                            </fields>
                        </widget>
                    </widgets>
                </dashboard>
            </dashboards>
        </template>
    </templates>
    <triggers>
        <trigger>
            <expression>{Asterisk PBX:check.asterisk[calls.longest.channel].str()}=1
and
{Asterisk PBX:check.asterisk[calls.longest.duration].last()}&gt;={$ASTERISK_CALLS_DURATION_WARN}</expression>
            <name>Call {ITEM.VALUE1} potentially stuck for {ITEM.VALUE2}</name>
            <priority>WARNING</priority>
        </trigger>
    </triggers>
